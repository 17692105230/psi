	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
/* 单行文本溢出显示... */
.textOverflow{
  overflow: hidden;text-overflow:ellipsis;white-space:nowrap;
}
.maxW78{
